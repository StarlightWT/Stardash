{
  "name": "stardash",
  "version": "1.0.0",
  "description": "Chaster dashboard",
  "main": "./main.js",
  "scripts": {
    "start": "npx electronmon .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.stardash.StarDash",
    "productName": "StarDash",
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    }
  },
  "publish": {
    "provider": "github",
    "releaseType": "relaese"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StarlightWT/Stardash.git"
  },
  "keywords": [],
  "author": "Starlight <starlightwt@protonmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StarlightWT/Stardash/issues"
  },
  "homepage": "https://github.com/StarlightWT/Stardash#readme",
  "devDependencies": {
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "electron-updater": "^6.1.4"
  }
}
