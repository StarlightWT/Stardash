{
	"name": "stardash",
	"version": "1.0.8",
	"description": "Chaster dashboard",
	"main": "./main.js",
	"scripts": {
		"start": "npx electronmon .",
		"dist": "electron-builder -w"
	},
	"build": {
		"appId": "com.stardash.StarDash",
		"productName": "stardash",
		"linux": {
			"target": "deb"
		},
		"win": {
			"target": "nsis"
		},
		"directories": {
			"output": "dist"
		}
	},
	"publish": {
		"provider": "github"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/StarlightWT/Stardash.git"
	},
	"keywords": [],
	"author": "Starlight <starlightwt@protonmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/StarlightWT/Stardash/issues"
	},
	"homepage": "https://github.com/StarlightWT/Stardash#readme",
	"devDependencies": {
		"electron": "^26.4.0",
		"electron-builder": "^24.6.4"
	},
	"dependencies": {
		"auto-launch": "^5.0.6",
		"axios": "^1.5.0",
		"electron-store": "^8.1.0",
		"electron-updater": "^6.1.4",
		"mongoose": "^7.5.3"
	}
}
